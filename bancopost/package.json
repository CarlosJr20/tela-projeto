{
  "name": "login-register-backend",
  "version": "1.0.0",
  "description": "Backend para sistema de login e cadastro com PostgreSQL",
  "main": "../dist/server.js",
  "scripts": {
    "start": "node ../bancopost/dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "build": "tsc",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate --config sequelize-config.js --migrations-path src/migrations"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.7",
    "@types/validator": "^13.11.1",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
